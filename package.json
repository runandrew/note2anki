{
	"name": "note2anki",
	"version": "0.3.0",
	"description": "Convert your Obsidian notes into Anki cards",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
