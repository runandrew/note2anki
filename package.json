		"test": "jest",
		"test:watch": "jest --watch"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.0",
		"@types/lodash": "^4.17.10",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint-plugin-import": "^2.31.0",
		"obsidian": "latest",
		"jest": "^29.5.0",
		"ts-jest": "^29.1.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"gray-matter": "^4.0.3",
		"lodash": "^4.17.21",
		"marked": "^14.1.2",
		"zod": "^3.23.8"
